/* pages/customer/cuslist/cuslist.wxss */
.search-header{
    position: fixed;
    top: 0;
    width: 750rpx;
    height: 182rpx;
    /* display: flex; */
    background: #fff;
    border-bottom: 1px solid rgba(0,0,0,.15);
    padding: 0 31.25rpx;
    font-size: 29rpx;
    color: #333;
    z-index: 100;
}

.search-header .input-box{
    position: relative;
    margin-top: 16rpx;
    float: left;
    width: 50%; 
    height: 59rpx;
    line-height: 59rpx;
    padding: 0 20rpx;
    background: #f4f4f4;
    border-radius: 8rpx;
}

.search-header .icon{
    position: absolute;
    top: 14rpx;
    left: 20rpx;
    width: 31rpx;
    height: 31rpx;
}

.search-header .del{
    position: absolute;
    top: 3rpx;
    right: 10rpx;
    width: 53rpx;
    height: 53rpx;
    z-index: 10;
}

.search-header .keywrod{
    position: absolute;
    top: 0;
    left: 40rpx;
    width: 210rpx;
    height: 59rpx;
    padding-left: 30rpx;
}

.search-header .right{
    margin-top: 24rpx;
    margin-left: 31rpx;
    margin-right: 6rpx;
    width: 58rpx;
    height: 43rpx;
    line-height: 43rpx;
    float: right;
    color: #b4282d;
}
.btnBox{
  width: 50%;
  float: left;
  display: flex;
  flex-flow: row nowrap;
  align-items:center;
  justify-content:space-between;
}
.search-header .btnBox .commoncol{
    height:58rpx;
    line-height:58rpx;
    margin-left:5%;
    margin-top:16rpx; 
    font-size:15px;
}
.search-header .selValue{
    display:flex;
    height:80rpx;
    width:100%;
    flex-flow:row nowrap;
    align-items:center;
    justify-content:space-between;
    margin-top: 10rpx;
}
.search-header .selValue .commoncol{ 
  height: 65rpx;
  line-height: 65rpx;
  margin-top: 28rpx;
  font-size: 14px;
}
.num{
  font-size: 14px;
  color: #b4282d;
}
.wselcolor{
  color: #666;
}
.selcolor{
  border:1px solid #b4282d;
  color: #b4282d;
}
.search-header .selValue button:nth-child(1){
  margin-left: 0;
}
.khname{
  margin-top: 16rpx;
  background: #f4f4f4;
  height: 59rpx;
  line-height:59rpx;
  margin-left:20rpx;
  padding: 0 20rpx;
  border-radius: 8rpx;
}
.cus-list{
  width: 100%;
  background: #ffffff;
  margin-top: 200rpx;
}
.icon_ywh{
  position:absolute;
  height:60px;
  width:60px;
  display:block;
  top:20px;
  right:20px;
}
.cusBox{
  width: 96%;
  margin: 0 auto;
  margin-bottom: 20rpx;
  /* border: 1px dashe d #999; */
  border-radius: 10rpx;
  position: relative;
}
.common{
  display:flex;
  flex-flow:row nowrap;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.common .cuslist{
  display:flex;
  flex-flow:row nowrap;
  justify-content: space-between;
  align-items: center; 
}
.cusli .li{
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
}
.cusdb{
  width: 100%;
  padding: 0 20rpx;
}
.cusli_n{
  display:flex;
  flex-flow:row nowrap;
  justify-content: space-between;
  align-items: center; 
}
.cusli_n .li{
  height: 80rpx;
  line-height: 80rpx;
  color: #666;
}
.addsty{
  position:fixed;
  right:20rpx;
  bottom:150rpx;
  z-index:200;
  width:160rpx;
  background:#000;
  opacity:0.5;
  height:50rpx;
  line-height:49rpx;
  text-align:center;
  border-radius:20rpx;
  color:#fff;
}
.empty-view{
  background: #ffffff;
}
.loadmore {
  height: 100rpx;
  width: 100%;
  line-height: 80rpx;
  text-align: center;
  margin-top: 0rpx;
}

.loadmore text {
  color: #999;
} 

@keyframes loading {
  0% {transform: rotate(0deg)}
  50% {transform: rotate(180deg)}
  100% {transform: rotate(360deg)}
}

.loading {
  display: inline-block;
  transform-origin: 50% 50%;
  animation: loading 1s linear infinite;
}

